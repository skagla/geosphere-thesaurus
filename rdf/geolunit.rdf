	<skos:definition xml:lang="en">Coarse grained quartzites and quartzitic slates with 10 to 50 m in thickness. (Flügel &amp; Hubmann, 2000)</skos:definition>
	<dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2015-08-14T16:04:16.000Z</dcterms:created>
	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/900"/>
	<skos:prefLabel xml:lang="en">Hundsberg Member</skos:prefLabel>
