<rdf:Description rdf:about="https://resource.geosphere.at/thes/geolunit/2242">
	<skos:broader rdf:resource="https://resource.geosphere.at/thes/geolunit/2215"/>
	<dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2025-09-05T09:19:36.387Z</dcterms:created>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:prefLabel xml:lang="en">Schilt Formation</skos:prefLabel>
	<skos:definition xml:lang="de">Oxfordium; Dunkelgraue, häufig von Kalzit durchaderte Mergel bis Tonmergelsteine sind die charakteristischen Gesteine der Schilt-Formation. (aus Friebe et al., 2007, S. 84)</skos:definition>
	<skos:definition xml:lang="en">Oxfordium; dark grey marl to clay marl stones, often veined with calcite, are the characteristic rocks of the Schilt Formation. (from Friebe et al., 2007, p. 84)</skos:definition>
	<skos:broader rdf:resource="https://resource.geosphere.at/thes/geolunit/52"/>
</rdf:Description>

