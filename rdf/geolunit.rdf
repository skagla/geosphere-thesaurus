	<dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2025-02-17T10:47:45.014Z</dcterms:created>
	<skos:prefLabel xml:lang="en">Roßkogelporphyroid</skos:prefLabel>
