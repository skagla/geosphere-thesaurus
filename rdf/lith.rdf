	<skos:broadMatch rdf:resource="http://inspire.ec.europa.eu/codelist/LithologyValue/ashAndLapilli"/>
	<skos:prefLabel xml:lang="en">Lapilli tephra</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="https://resource.geosphere.at/thes/lith/166">
	<skos:narrower rdf:resource="https://resource.geosphere.at/thes/lith/232"/>
	<dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2012-03-12T12:03:50.000Z</dcterms:created>
	<skos:narrower rdf:resource="https://resource.geosphere.at/thes/lith/228"/>
	<skos:narrower rdf:resource="https://resource.geosphere.at/thes/lith/229"/>
	<skos:narrower rdf:resource="https://resource.geosphere.at/thes/lith/222"/>
	<skos:prefLabel xml:lang="de">Klastisches Sediment</skos:prefLabel>
	<skos:narrower rdf:resource="https://resource.geosphere.at/thes/lith/226"/>
	<skos:narrower rdf:resource="https://resource.geosphere.at/thes/lith/227"/>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:exactMatch rdf:resource="http://inspire.ec.europa.eu/codelist/LithologyValue/clasticSedimentaryMaterial"/>
	<skos:definition xml:lang="de">Sediment, welches durch die Akkumulierung von zerbrochenen Fragmenten gebildet wird, welche sich aus präexistierenden Gesteinen und Mineralen ableiten und als separate Partikel vor ihrer Ablagerung von mechanischen Kräften (wie Wind, Wasser, Eis oder Gravitation) transportiert wurden. Der Begriff beinhaltet karbonatisch-klastische Mischgesteine (verändert nach Neuendorf et al., 2005).</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/lithology/clastic_sediment"/>
	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/545"/>
	<skos:prefLabel xml:lang="en">Clastic sediment</skos:prefLabel>
	<dcterms:hasVersion rdf:datatype="http://www.w3.org/2001/XMLSchema#int">11</dcterms:hasVersion>
	<skos:narrower rdf:resource="https://resource.geosphere.at/thes/lith/218"/>
	<skos:narrower rdf:resource="https://resource.geosphere.at/thes/lith/213"/>
	<skos:narrower rdf:resource="https://resource.geosphere.at/thes/lith/214"/>
	<dcterms:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2011-03-22T14:52:39Z</dcterms:date>
	<skos:definition xml:lang="en">A sediment formed by the accumulation of broken fragments derived from pre-existing rocks or minerals and transported as separate particles by mechanical agents (such as wind, water, ice, gravity) prior to deposition. The term includes mixed carbonate-clastic sediments (modified from Neuendorf et al., 2005).</skos:definition>
	<skos:narrower rdf:resource="https://resource.geosphere.at/thes/lith/240"/>
	<colourHexCode xmlns="http://dbpedia.org/ontology/">#D9FDD3</colourHexCode>
</rdf:Description>

<rdf:Description rdf:about="https://resource.geosphere.at/thes/lith/234">
	<dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2012-03-12T10:40:46.000Z</dcterms:created>
	<dcterms:hasVersion rdf:datatype="http://www.w3.org/2001/XMLSchema#int">8</dcterms:hasVersion>
	<skos:definition xml:lang="de">Karbonatisch-klastisches Sediment, dessen klastischer Anteil sich auf die Sandfraktion beschränkt. Besteht aus 25–50 % Calcit und 50–75 % Sand (Füchtbauer, 1988).</skos:definition>
	<skos:related rdf:resource="https://resource.geosphere.at/thes/lith/194"/>
	<skos:broadMatch rdf:resource="http://inspire.ec.europa.eu/codelist/LithologyValue/sand"/>
	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/229"/>
	<dcterms:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2011-03-22T14:52:39Z</dcterms:date>
	<skos:prefLabel xml:lang="en">Calcareous sand</skos:prefLabel>
	<skos:related rdf:resource="https://resource.geosphere.at/thes/lith/235"/>
	<skos:definition xml:lang="en">A mixed carbonate-clastic rock in which the clastic portion is restricted to sand-sized components. Consists of 25-50% calcite and 50-75% sand (Füchtbauer, 1988).</skos:definition>
	<skos:closeMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/lithology/sand"/>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:prefLabel xml:lang="de">Kalksand</skos:prefLabel>
</rdf:Description>

