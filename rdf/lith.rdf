	<colourHexCode xmlns="http://dbpedia.org/ontology/">#CC59D9</colourHexCode>
	<skos:prefLabel xml:lang="en">Block tephra</skos:prefLabel>
	<skos:exactMatch rdf:resource="http://data.bgs.ac.uk/id/EarthMaterialClass/RockName/BLTPH"/>
	<skos:related rdf:resource="https://resource.geosphere.at/thes/lith/11"/>
	<skos:definition xml:lang="de">Unverfestigte pyroklastische Ablagerung, welche zu mehr als 75 % aus vulkanischen Blöcken (Durchmesser &gt; 64 mm) besteht (Le Maitre et al., 2005).</skos:definition>
	<skos:prefLabel xml:lang="de">Block-Tephra</skos:prefLabel>
	<skos:broadMatch rdf:resource="http://inspire.ec.europa.eu/codelist/LithologyValue/ashBrecciaBombOrBlockTephra"/>
	<dcterms:hasVersion rdf:datatype="http://www.w3.org/2001/XMLSchema#int">7</dcterms:hasVersion>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:broadMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/lithology/ash_breccia_bomb_or_block_tephra"/>
	<dcterms:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2011-03-15T15:22:10Z</dcterms:date>
	<skos:definition xml:lang="en">An unconsolidated pyroclastic deposit, more than 75% of which is composed of volcanic blocks (diameter &gt; 64 mm) (Le Maitre et al., 2005).</skos:definition>
	<dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2012-03-12T10:29:10.000Z</dcterms:created>
</rdf:Description>

<rdf:Description rdf:about="https://resource.geosphere.at/thes/lith/9">
	<skos:prefLabel xml:lang="de">Lapilli-Tephra</skos:prefLabel>
	<skos:broadMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/lithology/ash_and_lapilli"/>
