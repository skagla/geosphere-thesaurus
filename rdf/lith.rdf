<rdf:Description rdf:about="https://resource.geosphere.at/thes/lith/35">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2012-03-12T16:09:49.000Z</dcterms:created>
