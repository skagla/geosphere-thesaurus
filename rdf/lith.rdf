	<dcterms:hasVersion rdf:datatype="http://www.w3.org/2001/XMLSchema#int">12</dcterms:hasVersion>
	<skos:definition xml:lang="en">A clastic sedimentary rock that contains &gt; 50% rounded mineral and rock fragments with a median grain size between 20 and 63 mm (Füchtbauer, 1988; DIN 4022).</skos:definition>
	<skos:broadMatch rdf:resource="http://inspire.ec.europa.eu/codelist/LithologyValue/gravel"/>
