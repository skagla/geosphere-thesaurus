	<dcterms:hasVersion rdf:datatype="http://www.w3.org/2001/XMLSchema#int">7</dcterms:hasVersion>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
</rdf:Description>

<rdf:Description rdf:about="https://resource.geosphere.at/thes/lith/243">
	<dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2012-01-18T10:22:23.000Z</dcterms:created>
	<skos:definition xml:lang="en">A rock that consists of cellular or porous limestone or dolostone in which secondary dissolution of readily soluble material (gypsum/anhydrite) has formed cavities. Rauhwackes are mainly formed by sedimentary or tectonic processes (modified from Murawski &amp; Meyer, 1998).</skos:definition>
	<foaf:depiction rdf:resource="https://resource.geosphere.at/img/thes/Rauhwacke.jpg"/>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:prefLabel xml:lang="de">Rauwacke</skos:prefLabel>
	<skos:prefLabel xml:lang="en">Rauwacke</skos:prefLabel>
	<skos:broadMatch rdf:resource="http://inspire.ec.europa.eu/codelist/LithologyValue/carbonateSedimentaryRock"/>
	<skos:definition xml:lang="de">Gestein, welches aus zellig-porösem Kalk oder Dolomit besteht. Die Hohlräume sind durch sekundäre Auslaugung von leicht lösbaren Komponenten (Gips/Anhydrit) entstanden. Die primäre Entstehung dieser Gesteine umfasst sowohl sedimentäre als auch tektonische Bildungen. (verändert nach Murawski &amp; Meyer, 1998)</skos:definition>
	<skos:broadMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/lithology/carbonate_sedimentary_rock"/>
	<skos:altLabel xml:lang="de">Rauhwacke</skos:altLabel>
	<skos:altLabel xml:lang="en">Rauhwacke</skos:altLabel>
	<dcterms:hasVersion rdf:datatype="http://www.w3.org/2001/XMLSchema#int">18</dcterms:hasVersion>
	<skos:altLabel xml:lang="de">Zellenkalk</skos:altLabel>
	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/486"/>
	<skos:altLabel xml:lang="de">Rauchwacke</skos:altLabel>
	<skos:altLabel xml:lang="en">Rauchwacke</skos:altLabel>
</rdf:Description>

<rdf:Description rdf:about="https://resource.geosphere.at/thes/lith/161">
	<skos:prefLabel xml:lang="de">Klastisches Sedimentgestein</skos:prefLabel>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/lithology/clastic_sedimentary_rock"/>
	<dcterms:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2011-03-22T14:52:39Z</dcterms:date>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/545"/>
	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/547"/>
	<skos:narrower rdf:resource="https://resource.geosphere.at/thes/lith/181"/>
	<skos:narrower rdf:resource="https://resource.geosphere.at/thes/lith/182"/>
	<skos:narrower rdf:resource="https://resource.geosphere.at/thes/lith/183"/>
	<skos:narrower rdf:resource="https://resource.geosphere.at/thes/lith/173"/>
	<skos:narrower rdf:resource="https://resource.geosphere.at/thes/lith/177"/>
	<skos:definition xml:lang="de">Festgestein, welches aus &gt; 50 % klastischen Sedimenten (Fragmente von präexistierenden Gesteinen jeglicher Art, welche vor ihrer Ablagerung durch Wind, Wasser, Eis oder Gravitation transportiert wurden) besteht. Der Begriff beinhaltet karbonatisch-klastische Mischgesteine (verändert nach Neuendorf, 2005; SLTT Appendix C, 2004).</skos:definition>
	<skos:narrower rdf:resource="https://resource.geosphere.at/thes/lith/171"/>
	<skos:narrower rdf:resource="https://resource.geosphere.at/thes/lith/172"/>
	<skos:definition xml:lang="en">A consolidated sedimentary rock consisting of &gt; 50% clastic sediments (fragments of any kind of rock that have been transported by mechanical agents such as wind, water, ice or gravity prior to deposition).The term includes mixed carbonate-clastic sedimentary rocks (modified from Neuendorf et al., 2005; SLTT Appendix C, 2004).</skos:definition>
	<skos:narrower rdf:resource="https://resource.geosphere.at/thes/lith/190"/>
	<skos:narrower rdf:resource="https://resource.geosphere.at/thes/lith/192"/>
	<skos:narrower rdf:resource="https://resource.geosphere.at/thes/lith/194"/>
	<skos:exactMatch rdf:resource="http://inspire.ec.europa.eu/codelist/LithologyValue/clasticSedimentaryRock"/>
	<dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2012-03-12T12:08:25.000Z</dcterms:created>
	<colourHexCode xmlns="http://dbpedia.org/ontology/">#CCB266</colourHexCode>
	<dcterms:hasVersion rdf:datatype="http://www.w3.org/2001/XMLSchema#int">16</dcterms:hasVersion>
	<skos:prefLabel xml:lang="en">Clastic sedimentary rock</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="https://resource.geosphere.at/thes/lith/190">
	<skos:related rdf:resource="https://resource.geosphere.at/thes/lith/230"/>
	<skos:closeMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/lithology/impure_dolostone"/>
	<skos:definition xml:lang="de">Karbonatisch-klastisches Mischgestein, dessen klastischer Anteil sich auf die Tonfraktion beschränkt. Besteht aus 25 bis 75 % Ton und 25-75% Dolomit. Kommt im Vergleich zu kalkigen Mergeln nur untergeordnet vor (Vinx, 2005; Füchtbauer, 1988).</skos:definition>
	<skos:definition xml:lang="en">A mixed carbonate-clastic sedimentary rock in which the clastic portion is restricted to clay-sized components. Dolomitic marl consists of 25-75% clay and 25-75% of dolomite. Dolomitic marlstone is less common than calcareous marlstone (Vinx, 2005; Füchtbauer, 1988).</skos:definition>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dcterms:hasVersion rdf:datatype="http://www.w3.org/2001/XMLSchema#int">13</dcterms:hasVersion>
	<dcterms:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2011-03-22T14:52:39Z</dcterms:date>
	<skos:prefLabel xml:lang="en">Dolomitic marlstone</skos:prefLabel>
	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/229"/>
	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/878"/>
	<skos:prefLabel xml:lang="de">Dolomitischer Mergelstein</skos:prefLabel>
	<skos:broadMatch rdf:resource="http://inspire.ec.europa.eu/codelist/LithologyValue/clasticSedimentaryRock"/>
	<dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2012-03-12T14:23:58.000Z</dcterms:created>
	<colourHexCode xmlns="http://dbpedia.org/ontology/">#CCB266</colourHexCode>
</rdf:Description>

<rdf:Description rdf:about="https://resource.geosphere.at/thes/lith/182">
	<skos:relatedMatch rdf:resource="http://dbpedia.org/resource/Fault_breccia"/>
	<skos:related rdf:resource="https://resource.geosphere.at/thes/lith/13"/>
	<skos:related rdf:resource="https://resource.geosphere.at/thes/lith/12"/>
	<skos:altLabel xml:lang="de">Breccie</skos:altLabel>
	<skos:exactMatch rdf:resource="http://dbpedia.org/resource/Breccia"/>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/lithology/breccia"/>
	<colourHexCode xmlns="http://dbpedia.org/ontology/">#F86740</colourHexCode>
	<dcterms:hasVersion rdf:datatype="http://www.w3.org/2001/XMLSchema#int">14</dcterms:hasVersion>
	<dcterms:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2011-03-22T14:52:39Z</dcterms:date>
	<skos:prefLabel xml:lang="en">Breccia</skos:prefLabel>
	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/229"/>
	<skos:exactMatch rdf:resource="http://data.bgs.ac.uk/id/EarthMaterialClass/RockName/BREC"/>
	<skos:prefLabel xml:lang="de">Brekzie</skos:prefLabel>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2014-09-01T10:33:10.000Z</dcterms:created>
	<skos:definition xml:lang="en">A clastic sedimentary rock that contains &gt; 50% angular mineral and rock fragments with a median grain size &gt; 2mm. A breccia is the solidified equivalent of scree and is usually clast-supported. The matrix can contain sand- or clay-sized particles. The term includes mono- and polymict breccias, i.e. breccias with homogeneous or heterogeneous lithological components (Füchtbauer, 1988; Neuendorf et al., 2005).</skos:definition>
	<skos:exactMatch rdf:resource="http://inspire.ec.europa.eu/codelist/LithologyValue/breccia"/>
	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/545"/>
	<skos:definition xml:lang="de">Klastisches Sedimentgestein, welches zu &gt; 50 % aus nicht gerundeten Mineral- oder Gesteinsbruchstücken von &gt; 2 mm besteht. Stellt das verfestigte Äquivalent von Schutt dar. Die Matrix kann sandig bis tonig sein, zeigt jedoch meist komponentengestütztes Gefüge. Umfasst mono- und polymikte Brekzien, d.s. Brekzien mit lithologisch homogenem oder heterogenem Komponentenbestand (Füchtbauer, 1988; Neuendorf et al., 2005).</skos:definition>
	<skos:related rdf:resource="https://resource.geosphere.at/thes/lith/153"/>
</rdf:Description>

<rdf:Description rdf:about="https://resource.geosphere.at/thes/lith/171">
	<skos:related rdf:resource="https://resource.geosphere.at/thes/lith/172"/>
	<skos:related rdf:resource="https://resource.geosphere.at/thes/lith/213"/>
	<skos:exactMatch rdf:resource="http://data.bgs.ac.uk/id/EarthMaterialClass/RockName/CST"/>
	<skos:definition xml:lang="de">Ungeschichtetes, massiges, klastisches Sedimentgestein mit einer medianen Korngröße &lt; 0.002 mm (Füchtbauer, 1988; DIN 4022).</skos:definition>
	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/78"/>
	<skos:exactMatch rdf:resource="http://inspire.ec.europa.eu/codelist/LithologyValue/claystone"/>
	<dcterms:hasVersion rdf:datatype="http://www.w3.org/2001/XMLSchema#int">10</dcterms:hasVersion>
	<skos:prefLabel xml:lang="en">Claystone</skos:prefLabel>
	<colourHexCode xmlns="http://dbpedia.org/ontology/">#BF996B</colourHexCode>
	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/229"/>
	<skos:exactMatch rdf:resource="http://dbpedia.org/resource/Claystone"/>
	<dcterms:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2011-03-22T14:52:39Z</dcterms:date>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="en">A unbedded, massive, clastic sedimentary rock with a median grain size &lt; 0.002 mm (Füchtbauer; 1988; DIN 4022).</skos:definition>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/lithology/claystone"/>
	<skos:exactMatch rdf:resource="http://www.wikidata.org/entity/Q1932127"/>
	<dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2012-03-12T12:11:27.000Z</dcterms:created>
	<skos:prefLabel xml:lang="de">Tonstein</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="https://resource.geosphere.at/thes/lith/181">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dcterms:hasVersion rdf:datatype="http://www.w3.org/2001/XMLSchema#int">11</dcterms:hasVersion>
	<foaf:depiction rdf:resource="https://resource.geosphere.at/img/thes/Konglomerat.jpg"/>
	<skos:exactMatch rdf:resource="http://www.wikidata.org/entity/Q191704"/>
	<skos:altLabel xml:lang="de">Orthokonglomerat</skos:altLabel>
	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/229"/>
	<dcterms:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2011-03-22T14:52:39Z</dcterms:date>
	<skos:definition xml:lang="en">A clastic sedimentary rock that contains &gt; 50% rounded mineral and rock fragments with a median grain size &gt; 2mm (the consolidated equivalent of gravel). The matrix can contain sandy or clay-sized particles but the rock is usually clast-supported. The term includes monomict and polymict conglomerates, i.e. conglomerates with homogeneous or heterogeneous lithological components (Füchtbauer, 1988; Neuendorf et al., 2005).</skos:definition>
	<colourHexCode xmlns="http://dbpedia.org/ontology/">#CCBFA6</colourHexCode>
	<skos:related rdf:resource="https://resource.geosphere.at/thes/lith/222"/>
	<skos:prefLabel xml:lang="de">Konglomerat</skos:prefLabel>
	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/545"/>
	<skos:prefLabel xml:lang="en">Conglomerate</skos:prefLabel>
	<skos:exactMatch rdf:resource="http://data.bgs.ac.uk/id/EarthMaterialClass/RockName/CONG"/>
	<skos:exactMatch rdf:resource="http://inspire.ec.europa.eu/codelist/LithologyValue/conglomerate"/>
	<skos:definition xml:lang="de">Klastisches Sedimentgestein, welches zu &gt; 50 % aus gerundeten Mineral- oder Gesteinsbruchstücken von &gt; 2 mm besteht. Stellt das verfestigte Äquivalent von Kies dar. Die Matrix kann sandig bis tonig sein, zeigt jedoch meist komponentengestütztes Gefüge. Umfasst mono- und polymikte Konglomerate, d.s. Konglomerate mit lithologisch homogenen oder heterogenen Komponenten (Füchtbauer, 1988; Neuendorf et al., 2005).</skos:definition>
	<dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2013-04-05T09:33:32.000Z</dcterms:created>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/lithology/clastic_conglomerate"/>
</rdf:Description>

<rdf:Description rdf:about="https://resource.geosphere.at/thes/lith/192">
	<dcterms:hasVersion rdf:datatype="http://www.w3.org/2001/XMLSchema#int">11</dcterms:hasVersion>
	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/878"/>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dcterms:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2011-03-22T14:52:39Z</dcterms:date>
	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/229"/>
	<skos:narrower rdf:resource="https://resource.geosphere.at/thes/lith/191"/>
	<skos:narrower rdf:resource="https://resource.geosphere.at/thes/lith/193"/>
	<skos:prefLabel xml:lang="de">Mergelstein</skos:prefLabel>
	<skos:broadMatch rdf:resource="http://inspire.ec.europa.eu/codelist/LithologyValue/clasticSedimentaryRock"/>
	<skos:closeMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/lithology/impure_carbonate_sedimentary_rock"/>
	<skos:definition xml:lang="de">Karbonatisch-klastisches Mischgestein, dessen klastischer Anteil sich auf die Tonfraktion beschränkt. Besteht aus 25–75 % Ton und 25–75 % Calcit (Vinx, 2005; Füchtbauer, 1988).</skos:definition>
	<skos:prefLabel xml:lang="en">Marlstone</skos:prefLabel>
	<dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2012-03-12T14:21:48.000Z</dcterms:created>
	<skos:related rdf:resource="https://resource.geosphere.at/thes/lith/232"/>
	<skos:definition xml:lang="en">A mixed carbonate-clastic rock in which the clastic portion is restricted to clay-sized components. Marlstone consists of 25-75% clay and 25-75% of calcite (Vinx, 2005; Füchtbauer, 1988).</skos:definition>
	<colourHexCode xmlns="http://dbpedia.org/ontology/">#CCB266</colourHexCode>
</rdf:Description>

<rdf:Description rdf:about="https://resource.geosphere.at/thes/lith/191">
	<skos:prefLabel xml:lang="en">Clay marlstone</skos:prefLabel>
	<skos:closeMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/lithology/impure_carbonate_sedimentary_rock"/>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:related rdf:resource="https://resource.geosphere.at/thes/lith/231"/>
	<skos:prefLabel xml:lang="de">Tonmergelstein</skos:prefLabel>
	<skos:definition xml:lang="de">Karbonatisch-klastisches Mischgestein, dessen klastischer Anteil sich auf die Tonfraktion beschränkt. Besteht aus 50–75 % Ton und Calcit (Vinx, 2005; Füchtbauer, 1988).</skos:definition>
	<skos:definition xml:lang="en">A mixed carbonate-clastic rock in which the clastic portion is restricted to clay-sized components. Clay marlstone consists of 50-75% clay and 25-50% calcite (Vinx, 2005; Füchtbauer, 1988).</skos:definition>
	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/878"/>
	<skos:broadMatch rdf:resource="http://inspire.ec.europa.eu/codelist/LithologyValue/clasticSedimentaryRock"/>
	<dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2012-03-12T12:10:03.000Z</dcterms:created>
	<dcterms:hasVersion rdf:datatype="http://www.w3.org/2001/XMLSchema#int">10</dcterms:hasVersion>
	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/229"/>
	<dcterms:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2011-03-22T14:52:39Z</dcterms:date>
</rdf:Description>

<rdf:Description rdf:about="https://resource.geosphere.at/thes/lith/193">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dcterms:hasVersion rdf:datatype="http://www.w3.org/2001/XMLSchema#int">11</dcterms:hasVersion>
	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/878"/>
	<skos:broadMatch rdf:resource="http://inspire.ec.europa.eu/codelist/LithologyValue/clasticSedimentaryRock"/>
	<skos:prefLabel xml:lang="de">Kalkmergelstein</skos:prefLabel>
	<skos:closeMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/lithology/impure_carbonate_sedimentary_rock"/>
	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/229"/>
	<dcterms:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2011-03-22T14:52:39Z</dcterms:date>
	<skos:definition xml:lang="en">A mixed carbonate-clastic rock in which the clastic portion is restricted to clay-sized components. Consists of 25-50% clay and 50-75% calcite (Vinx, 2005; Füchtbauer, 1988).</skos:definition>
	<skos:related rdf:resource="https://resource.geosphere.at/thes/lith/233"/>
	<dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2012-03-12T10:39:48.000Z</dcterms:created>
	<skos:definition xml:lang="de">Karbonatisch-klastisches Mischgestein, dessen klastischer Anteil sich auf die Tonfraktion beschränkt. Besteht aus 25–50 % Ton und Calcit (Vinx, 2005; Füchtbauer, 1988).</skos:definition>
	<skos:prefLabel xml:lang="en">Calcareous marlstone</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="https://resource.geosphere.at/thes/lith/173">
	<skos:related rdf:resource="https://resource.geosphere.at/thes/lith/214"/>
	<skos:exactMatch rdf:resource="http://www.wikidata.org/entity/Q565731"/>
	<skos:exactMatch rdf:resource="http://inspire.ec.europa.eu/codelist/LithologyValue/siltstone"/>
	<skos:altLabel xml:lang="de">Schluffstein</skos:altLabel>
	<skos:definition xml:lang="de">Klastisches Sedimentgestein mit einer medianen Korngröße &gt; 0.002 mm und ≤ 0.063 mm (Füchtbauer, 1988; DIN 4022).</skos:definition>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:narrower rdf:resource="https://resource.geosphere.at/thes/lith/175"/>
	<skos:narrower rdf:resource="https://resource.geosphere.at/thes/lith/174"/>
	<skos:narrower rdf:resource="https://resource.geosphere.at/thes/lith/176"/>
	<skos:definition xml:lang="en">A clastic sedimentary rock with a median grain size between 0.002 and 0.063 mm (Füchtbauer, 1988, DIN 4022).</skos:definition>
	<skos:exactMatch rdf:resource="http://dbpedia.org/resource/Siltstone"/>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/lithology/siltstone"/>
	<skos:prefLabel xml:lang="de">Siltstein</skos:prefLabel>
	<skos:exactMatch rdf:resource="http://data.bgs.ac.uk/id/EarthMaterialClass/RockName/SLST"/>
	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/78"/>
	<dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2012-03-12T16:23:26.000Z</dcterms:created>
	<dcterms:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2011-03-22T14:52:39Z</dcterms:date>
	<skos:prefLabel xml:lang="en">Siltstone</skos:prefLabel>
	<colourHexCode xmlns="http://dbpedia.org/ontology/">#CCA680</colourHexCode>
	<dcterms:hasVersion rdf:datatype="http://www.w3.org/2001/XMLSchema#int">12</dcterms:hasVersion>
	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/229"/>
</rdf:Description>

<rdf:Description rdf:about="https://resource.geosphere.at/thes/lith/175">
	<skos:prefLabel xml:lang="de">Mittelsiltstein</skos:prefLabel>
	<skos:altLabel xml:lang="de">Mittelschluffstein</skos:altLabel>
	<skos:definition xml:lang="en">A clastic sedimentary rock with a median grain size between 0.0063 and 0.02 mm (Füchtbauer, 1988; DIN 4022).</skos:definition>
