	<skos:related rdf:resource="https://resource.geosphere.at/thes/lith/34"/>
	<skos:exactMatch rdf:resource="http://data.bgs.ac.uk/id/EarthMaterialClass/RockName/SY"/>
	<foaf:depiction rdf:resource="https://resource.geosphere.at/img/thes/AKL_Museum_Syenite.jpg"/>
	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/477"/>
	<dcterms:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2011-03-15T15:22:10Z</dcterms:date>
	<skos:exactMatch rdf:resource="http://inspire.ec.europa.eu/codelist/LithologyValue/syenite"/>
