	<skos:prefLabel xml:lang="de">Asche</skos:prefLabel>
	<dcterms:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2011-03-15T15:22:10Z</dcterms:date>
	<skos:exactMatch rdf:resource="http://dbpedia.org/resource/Volcanic_ash"/>
	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/477"/>
	<skos:prefLabel xml:lang="en">Ash</skos:prefLabel>
	<skos:exactMatch rdf:resource="http://data.bgs.ac.uk/id/EarthMaterialClass/RockName/ASH"/>
	<skos:definition xml:lang="en">Unconsolidated pyroclastic deposit in which &gt;75% of the material has a grain size &lt; 2 mm (Le Maitre et al., 2005).</skos:definition>
	<dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2012-03-12T10:15:26.000Z</dcterms:created>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="de">Unverfestigte pyroklastische Ablagerung, welche zu mehr als 75 % aus Material mit Durchmesser &lt; 2 mm besteht (Le Maitre et al., 2005).</skos:definition>
	<skos:related rdf:resource="https://resource.geosphere.at/thes/lith/16"/>
	<colourHexCode xmlns="http://dbpedia.org/ontology/">#B24DCC</colourHexCode>
</rdf:Description>

<rdf:Description rdf:about="https://resource.geosphere.at/thes/lith/8">
