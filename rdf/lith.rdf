	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:related rdf:resource="https://resource.geosphere.at/thes/lith/234"/>
	<dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2012-03-12T10:41:26.000Z</dcterms:created>
