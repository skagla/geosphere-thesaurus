</rdf:Description>

<rdf:Description rdf:about="https://resource.geosphere.at/thes/lith/36">
	<skos:exactMatch rdf:resource="http://inspire.ec.europa.eu/codelist/LithologyValue/monzonite"/>
	<skos:definition xml:lang="en">A plutonic rock consisting of approximately equal amounts of plagioclase and alkali feldspar, with minor amphibole and/or pyroxene. Defined modally in QAPF-field 8 (Le Maitre et al., 2005).</skos:definition>
	<skos:related rdf:resource="https://resource.geosphere.at/thes/lith/35"/>
	<skos:related rdf:resource="https://resource.geosphere.at/thes/lith/37"/>
	<skos:exactMatch rdf:resource="http://dbpedia.org/resource/Monzonite"/>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/lithology/monzonite"/>
	<skos:related rdf:resource="https://resource.geosphere.at/thes/lith/80"/>
	<colourHexCode xmlns="http://dbpedia.org/ontology/">#F259D4</colourHexCode>
	<skos:exactMatch rdf:resource="http://www.wikidata.org/entity/Q1130233"/>
	<dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2012-03-12T14:35:34.000Z</dcterms:created>
	<skos:definition xml:lang="de">Plutonisches Gestein, welches zu gleichen Anteilen aus Plagioklas und Alkalifeldspat mit wenig Amphibol und/oder Pyroxen besteht. Modal definiert in QAPF-Feld 8 (Le Maitre et al., 2005).</skos:definition>
	<skos:prefLabel xml:lang="de">Monzonit</skos:prefLabel>
	<dcterms:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2011-03-15T15:22:10Z</dcterms:date>
	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/477"/>
	<dcterms:hasVersion rdf:datatype="http://www.w3.org/2001/XMLSchema#int">8</dcterms:hasVersion>
	<skos:prefLabel xml:lang="en">Monzonite</skos:prefLabel>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:exactMatch rdf:resource="http://data.bgs.ac.uk/id/EarthMaterialClass/RockName/MZ"/>
</rdf:Description>

<rdf:Description rdf:about="https://resource.geosphere.at/thes/lith/33">
