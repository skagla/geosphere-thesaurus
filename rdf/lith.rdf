	<skos:broadMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/lithology/mica_schist"/>
	<skos:definition xml:lang="en">A mica-schist that contains andalusite.</skos:definition>
</rdf:Description>

<rdf:Description rdf:about="https://resource.geosphere.at/thes/lith/246">
	<dcterms:hasVersion rdf:datatype="http://www.w3.org/2001/XMLSchema#int">9</dcterms:hasVersion>
