	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/477"/>
	<dcterms:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2011-03-15T15:22:10Z</dcterms:date>
</rdf:Description>

<rdf:Description rdf:about="https://resource.geosphere.at/thes/lith/41">
	<dcterms:hasVersion rdf:datatype="http://www.w3.org/2001/XMLSchema#int">8</dcterms:hasVersion>
	<skos:prefLabel xml:lang="de">Quarz-Monzogabbro</skos:prefLabel>
	<skos:definition xml:lang="en">A plutonic rock consisting essentially of calcic plagioclase, alkali feldspar, mafic minerals and quartz. Defined modally in QAPF-field 9* (Le Maitre et al., 2005).</skos:definition>
	<dcterms:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2011-03-15T15:22:10Z</dcterms:date>
	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/477"/>
