	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:definition xml:lang="de">Plutonisches Gestein, welches aus Kalzium-Plagioklas, Alkalifeldspat, mafischen Mineralen und Quarz besteht. Modal definiert in QAPF-Feld 9* (Le Maitre et al., 2005).</skos:definition>
	<dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2012-03-12T16:09:00.000Z</dcterms:created>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/lithology/quartz_monzogabbro"/>
	<skos:prefLabel xml:lang="en">Quartz monzogabbro</skos:prefLabel>
</rdf:Description>

