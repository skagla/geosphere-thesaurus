	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:broadMatch rdf:resource="http://inspire.ec.europa.eu/codelist/LithologyValue/micaSchist"/>
	<dcterms:hasVersion rdf:datatype="http://www.w3.org/2001/XMLSchema#int">8</dcterms:hasVersion>
	<skos:prefLabel xml:lang="de">Granat-Glimmerschiefer</skos:prefLabel>
	<skos:definition xml:lang="de">Glimmerschiefer der Granat führt.</skos:definition>
</rdf:Description>

<rdf:Description rdf:about="https://resource.geosphere.at/thes/lith/247">
	<skos:broadMatch rdf:resource="http://inspire.ec.europa.eu/codelist/LithologyValue/micaSchist"/>
	<skos:prefLabel xml:lang="de">Andalusit-Glimmerschiefer</skos:prefLabel>
	<skos:definition xml:lang="de">Glimmerschiefer der Andalusit führt.</skos:definition>
	<dcterms:hasVersion rdf:datatype="http://www.w3.org/2001/XMLSchema#int">13</dcterms:hasVersion>
