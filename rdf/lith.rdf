	<skos:narrower rdf:resource="https://resource.geosphere.at/thes/lith/239"/>
	<skos:narrower rdf:resource="https://resource.geosphere.at/thes/lith/238"/>
	<skos:closeMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/lithology/organic_rich_sedimentary_material"/>
	<dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2012-03-12T15:51:04.000Z</dcterms:created>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:related rdf:resource="https://resource.geosphere.at/thes/lith/164"/>
	<colourHexCode xmlns="http://dbpedia.org/ontology/">#42413C</colourHexCode>
</rdf:Description>

<rdf:Description rdf:about="https://resource.geosphere.at/thes/lith/238">
	<skos:exactMatch rdf:resource="http://inspire.ec.europa.eu/codelist/LithologyValue/peat"/>
	<skos:definition xml:lang="de">Unverfestigte Ablagerung aus Pflanzenresten mit hohem Wasseranteil (≥ 75 %). Der Kohlenstoffanteil beträgt rund 60 % und der Sauerstoffanteil liegt bei 30 % im trockenen Zustand. Wird durch Inkohlung zu Braunkohle (Neuendorf et al., 2005; Tucker, 2001).</skos:definition>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/lithology/peat"/>
	<colourHexCode xmlns="http://dbpedia.org/ontology/">#E6D994</colourHexCode>
	<skos:definition xml:lang="en">An unconsolidated deposit of plant remains with a high water content (≥ 75%), a fixed carbon content of about 60%, and an oxygen content of about 30% (on a moist mineral free basis). Forms lignite during coalification (Neuendorf et al., 2005; Tucker, 2001).</skos:definition>
	<skos:prefLabel xml:lang="en">Peat</skos:prefLabel>
	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/545"/>
	<skos:exactMatch rdf:resource="http://dbpedia.org/resource/Peat"/>
	<skos:exactMatch rdf:resource="http://www.wikidata.org/entity/Q184624"/>
	<skos:exactMatch rdf:resource="http://data.bgs.ac.uk/id/EarthMaterialClass/RockName/PEAT"/>
	<skos:prefLabel xml:lang="de">Torf</skos:prefLabel>
	<dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2012-03-12T15:53:20.000Z</dcterms:created>
	<dcterms:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2011-03-22T14:52:39Z</dcterms:date>
	<dcterms:hasVersion rdf:datatype="http://www.w3.org/2001/XMLSchema#int">11</dcterms:hasVersion>
	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/842"/>
</rdf:Description>

<rdf:Description rdf:about="https://resource.geosphere.at/thes/lith/239">
	<skos:exactMatch rdf:resource="http://dbpedia.org/resource/Sapropel"/>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/lithology/sapropel"/>
	<skos:definition xml:lang="de">Unverfestigter, gallertiger Schlamm, der sich aus Pflanzenresten zusammensetzt (meistens Algen) und unter anaeroben Bedingungen verfault. Gebildet am Meeresgrund bzw. am Grund von Seen (Neuendorf et al., 2005).</skos:definition>
	<dcterms:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2011-03-22T14:52:39Z</dcterms:date>
	<skos:exactMatch rdf:resource="http://inspire.ec.europa.eu/codelist/LithologyValue/sapropel"/>
	<colourHexCode xmlns="http://dbpedia.org/ontology/">#EBDE9E</colourHexCode>
	<skos:definition xml:lang="en">An unconsolidated jelly-like ooze composed of plant remains (most often algae), putrefying under anaerobic conditions on the shallow lake and sea floor (Neuendorf et al., 2005).</skos:definition>
	<skos:altLabel xml:lang="de">Faulschlamm</skos:altLabel>
	<dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2012-03-12T16:16:26.000Z</dcterms:created>
	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/545"/>
	<skos:prefLabel xml:lang="de">Sapropel</skos:prefLabel>
	<skos:prefLabel xml:lang="en">Sapropel</skos:prefLabel>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dcterms:hasVersion rdf:datatype="http://www.w3.org/2001/XMLSchema#int">9</dcterms:hasVersion>
</rdf:Description>

<rdf:Description rdf:about="https://resource.geosphere.at/thes/lith/170">
	<colourHexCode xmlns="http://dbpedia.org/ontology/">#99CCE6</colourHexCode>
	<skos:prefLabel xml:lang="en">Chemical sediment</skos:prefLabel>
	<dcterms:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2011-03-22T14:52:39Z</dcterms:date>
	<dcterms:hasVersion rdf:datatype="http://www.w3.org/2001/XMLSchema#int">11</dcterms:hasVersion>
	<skos:closeMatch rdf:resource="http://inspire.ec.europa.eu/codelist/LithologyValue/chemicalSedimentaryMaterial"/>
	<skos:definition xml:lang="en">A sediment composed primarily of material formed directly by precipitation from solution or colloidal suspension (evaporation), or by deposition of insoluble precipitates (mixing of insoluble salts). (Neuendorf et al., 2005)</skos:definition>
	<skos:prefLabel xml:lang="de">Chemisches Sediment</skos:prefLabel>
	<dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2012-03-12T11:58:51.000Z</dcterms:created>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/545"/>
	<skos:closeMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/lithology/chemical_sedimentary_material"/>
	<skos:definition xml:lang="de">Sediment, das hauptsächlich aus Material besteht, welches durch Fällung aus einer Lösung oder einer kolloidalen Suspension (Evaporation) oder durch Ablagerung von unlöslichen Abscheidungsprodukten (Mischung von unlöslichen Salzen) entstanden ist. (Neuendorf et al., 2005)</skos:definition>
</rdf:Description>

<rdf:Description rdf:about="https://resource.geosphere.at/thes/lith/209">
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/lithology/evaporite"/>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<colourHexCode xmlns="http://dbpedia.org/ontology/">#99CCE6</colourHexCode>
	<dcterms:hasVersion rdf:datatype="http://www.w3.org/2001/XMLSchema#int">14</dcterms:hasVersion>
	<skos:exactMatch rdf:resource="http://inspire.ec.europa.eu/codelist/LithologyValue/evaporite"/>
	<skos:definition xml:lang="en">A chemical sedimentary rock containing &gt; 50% non-carbonate salts, which can include chlorite, sulphate, or borate minerals. Usually formed from a saline solution by precipitation due to vaporization of the solvent (Neuendorf et al., 2005; SLTT Appendix C, 2004).</skos:definition>
	<skos:narrower rdf:resource="https://resource.geosphere.at/thes/lith/212"/>
	<skos:narrower rdf:resource="https://resource.geosphere.at/thes/lith/211"/>
	<skos:narrower rdf:resource="https://resource.geosphere.at/thes/lith/210"/>
	<skos:prefLabel xml:lang="de">Evaporit</skos:prefLabel>
	<dcterms:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2011-03-22T14:52:39Z</dcterms:date>
	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/545"/>
	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/547"/>
	<skos:definition xml:lang="de">Chemisches Sedimentgestein, welches &gt; 50 % nicht-karbonatische Minerale wie Chlorid-, Nitrat-, Sulfat- oder Boratminerale enthält. Typischerweise durch Fällung aus einer salzigen Lösung infolge Verdunstung des Lösungsmittels gebildet (Neuendorf et al., 2005; SLTT Appendix C, 2004).</skos:definition>
	<skos:prefLabel xml:lang="en">Evaporite</skos:prefLabel>
	<dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2012-03-12T12:53:27.000Z</dcterms:created>
	<skos:exactMatch rdf:resource="http://dbpedia.org/resource/Evaporite"/>
	<skos:exactMatch rdf:resource="http://www.wikidata.org/entity/Q491689"/>
</rdf:Description>

<rdf:Description rdf:about="https://resource.geosphere.at/thes/lith/212">
	<skos:definition xml:lang="en">An evaporite composed of &gt; 50% anhydrite (CaSO4). Often associated with gypsum and halite , forming thick interbedded layers with limestone and shale (Neuendorf et al., 2005).</skos:definition>
	<skos:broadMatch rdf:resource="http://inspire.ec.europa.eu/codelist/LithologyValue/gypsumOrAnhydrite"/>
	<skos:prefLabel xml:lang="de">Anhydrit</skos:prefLabel>
	<skos:related rdf:resource="https://resource.geosphere.at/thes/lith/211"/>
	<skos:definition xml:lang="de">Evaporit, der sich zu &gt; 50% aus Anhydrit (CaSO4) zusammensetzt. Bildet oft im Verbund mit Gips und Halit dicke Einschaltungen in Kalkstein und Schieferton (Neuendorf et al., 2005).</skos:definition>
	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/545"/>
	<dcterms:hasVersion rdf:datatype="http://www.w3.org/2001/XMLSchema#int">7</dcterms:hasVersion>
	<foaf:depiction rdf:resource="https://resource.geosphere.at/img/thes/Anhydrit.jpg"/>
	<dcterms:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2011-03-22T14:52:39Z</dcterms:date>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:exactMatch rdf:resource="http://dbpedia.org/resource/Anhydrite"/>
	<skos:prefLabel xml:lang="en">Anhydrite</skos:prefLabel>
	<dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2013-04-05T09:22:56.000Z</dcterms:created>
	<skos:broadMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/lithology/rock_gypsum_or_anhydrite"/>
	<colourHexCode xmlns="http://dbpedia.org/ontology/">#B2E6F2</colourHexCode>
</rdf:Description>

<rdf:Description rdf:about="https://resource.geosphere.at/thes/lith/211">
	<skos:prefLabel xml:lang="en">Gypsum</skos:prefLabel>
	<skos:exactMatch rdf:resource="http://data.bgs.ac.uk/id/EarthMaterialClass/RockName/GYPS"/>
	<dcterms:hasVersion rdf:datatype="http://www.w3.org/2001/XMLSchema#int">8</dcterms:hasVersion>
	<dcterms:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2011-03-22T14:52:39Z</dcterms:date>
	<skos:definition xml:lang="en">An evaporite composed of &gt; 50% gypsum (CaSO4 • 2H2O), often associated with anhydrite and halite, forming thick interbedded layers with limestone and shale (Neuendorf et al., 2005).</skos:definition>
	<skos:related rdf:resource="https://resource.geosphere.at/thes/lith/212"/>
	<foaf:depiction rdf:resource="https://resource.geosphere.at/img/thes/Gips.jpg"/>
	<skos:definition xml:lang="de">Evaporit, der sich zu &gt; 50 % aus Gips (CaSO4 • 2H2O) zusammensetzt. Bildet oft im Verbund mit Anhydrit und Halit dicke Einschaltungen in Kalkstein und Schieferton (Neuendorf et al., 2005).</skos:definition>
	<skos:exactMatch rdf:resource="http://dbpedia.org/resource/Gypsum"/>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:broadMatch rdf:resource="http://inspire.ec.europa.eu/codelist/LithologyValue/gypsumOrAnhydrite"/>
	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/545"/>
	<dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2013-04-05T09:27:13.000Z</dcterms:created>
	<skos:broadMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/lithology/rock_gypsum_or_anhydrite"/>
	<colourHexCode xmlns="http://dbpedia.org/ontology/">#B2E6F2</colourHexCode>
	<skos:prefLabel xml:lang="de">Gips</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="https://resource.geosphere.at/thes/lith/210">
	<dcterms:hasVersion rdf:datatype="http://www.w3.org/2001/XMLSchema#int">6</dcterms:hasVersion>
	<skos:exactMatch rdf:resource="http://inspire.ec.europa.eu/codelist/LithologyValue/rockSalt"/>
	<skos:definition xml:lang="de">Evaporit, der sich zu &gt; 50 % aus (grobkristallinem) Halit (NaCl) zusammensetzt. Typischerweise durch Eisenoxide gefärbt und mit feinkörnigen Sedimenten (Ton) vermischt.</skos:definition>
	<dcterms:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2011-03-22T14:52:39Z</dcterms:date>
	<skos:definition xml:lang="en">An evaporite composed of &gt; 50% crystalline halite (NaCl), typically stained by iron-oxides and mixed with fine-grained sediments (clay) (Neuendorf et al., 2005).</skos:definition>
	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/545"/>
	<skos:exactMatch rdf:resource="http://www.wikidata.org/entity/Q2341557"/>
	<dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2013-04-05T09:41:54.000Z</dcterms:created>
	<colourHexCode xmlns="http://dbpedia.org/ontology/">#AADAF2</colourHexCode>
	<foaf:depiction rdf:resource="https://resource.geosphere.at/img/thes/Steinsalz.jpg"/>
	<skos:prefLabel xml:lang="de">Steinsalz</skos:prefLabel>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:prefLabel xml:lang="en">Rock salt</skos:prefLabel>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/lithology/rock_salt"/>
</rdf:Description>

<rdf:Description rdf:about="https://resource.geosphere.at/thes/lith/4">
	<skos:definition xml:lang="de">Überwiegend unverfestigte, pyroklastische Ablagerung, welche zu mehr als 75 Vol.-% aus Pyroklasten besteht (Le Maitre et al., 2005).</skos:definition>
	<skos:exactMatch rdf:resource="http://dbpedia.org/resource/Tephra"/>
	<colourHexCode xmlns="http://dbpedia.org/ontology/">#B24DCC</colourHexCode>
	<skos:prefLabel xml:lang="de">Tephra</skos:prefLabel>
	<skos:prefLabel xml:lang="en">Tephra</skos:prefLabel>
	<skos:definition xml:lang="en">Predominantly unconsolidated pyroclastic deposits that contain more than 75% pyroclasts (by volume) (Le Maitre et al., 2005).</skos:definition>
	<skos:narrower rdf:resource="https://resource.geosphere.at/thes/lith/9"/>
	<skos:narrower rdf:resource="https://resource.geosphere.at/thes/lith/8"/>
	<skos:exactMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/lithology/tephra"/>
	<dcterms:hasVersion rdf:datatype="http://www.w3.org/2001/XMLSchema#int">13</dcterms:hasVersion>
	<dcterms:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2011-03-15T15:22:10Z</dcterms:date>
	<dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2012-03-12T16:29:32.000Z</dcterms:created>
	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/477"/>
