<rdf:Description rdf:about="https://resource.geosphere.at/thes/lith/233">
	<skos:definition xml:lang="de">Karbonatisch-klastisches Sediment, dessen klastischer Anteil sich auf die Tonfraktion beschränkt. Besteht aus 25–50 % Ton und 50–75 % Calcit (Vinx, 2005; Füchtbauer, 1988).</skos:definition>
	<skos:closeMatch rdf:resource="http://inspire.ec.europa.eu/codelist/LithologyValue/impureCalcareousCarbonateSediment"/>
	<skos:closeMatch rdf:resource="http://resource.geosciml.org/classifier/cgi/lithology/impure_calcareous_carbonate_sediment"/>
	<dcterms:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2011-03-22T14:52:39Z</dcterms:date>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/229"/>
	<skos:related rdf:resource="https://resource.geosphere.at/thes/lith/193"/>
	<skos:prefLabel xml:lang="de">Kalkmergel</skos:prefLabel>
	<dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2012-03-12T10:39:09.000Z</dcterms:created>
	<dcterms:hasVersion rdf:datatype="http://www.w3.org/2001/XMLSchema#int">9</dcterms:hasVersion>
	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/878"/>
	<skos:definition xml:lang="en">A mixed carbonate-clastic sediment in which the clastic portion is restricted to clay-sized components. Consists of 25-50% clay and 50-75% calcite (Vinx, 2005; Füchtbauer, 1988).</skos:definition>
	<skos:prefLabel xml:lang="en">Calcareous marl</skos:prefLabel>
</rdf:Description>

