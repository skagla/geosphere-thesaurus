	<skos:broadMatch rdf:resource="http://inspire.ec.europa.eu/codelist/CommodityCodeValue/magnesite"/>
	<skos:example xml:lang="de">Magnesit-(Talk-)bezirk Veitsch-Decke (Veitsch)</skos:example>
	<skos:example xml:lang="de">Magnesit-(Eisenkarbonat-)bezirk Tirolisch-Norisches Deckensystem (Dienten)</skos:example>
	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/890"/>
	<skos:prefLabel xml:lang="en">Sparry Magnesite</skos:prefLabel>
	<dcterms:references rdf:resource="https://resource.geosphere.at/thes/citation/1149"/>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
