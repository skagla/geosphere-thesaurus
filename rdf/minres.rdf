<rdf:Description rdf:about="https://resource.geosphere.at/thes/minres/43">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:notation>Gabb</skos:notation>
	<dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2018-11-20T08:04:41.000Z</dcterms:created>
	<skos:prefLabel xml:lang="en">[Gabbro, Dolerit (Dekorsteine)]</skos:prefLabel>
	<skos:prefLabel xml:lang="de">Gabbro, Dolerit (Dekorsteine)</skos:prefLabel>
